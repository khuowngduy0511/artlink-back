{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/ArtLinkSerilog-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "MSSQLServerDB": "Data Source=.;Initial Catalog=ArtLinkDB;Integrated Security=True;Trust Server Certificate=true"
  }
}
